<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" maxHeight="600.0" maxWidth="1024.0" prefHeight="600.0" prefWidth="1024.0" stylesheets="@../app.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rk.rynkbit.can.presenter.speedometer.SpeedometerController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox fx:id="toolBox" onMousePressed="#clickHBox" prefHeight="20.0" spacing="10.0" VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <children>
                  <Label fx:id="labelVersion" text="0.0.1" textFill="WHITE" />
                  <Label text="Messages: " textFill="WHITE" />
                  <Label fx:id="labelMessageCount" textFill="WHITE" />
               </children>
            </HBox>
            <HBox fx:id="primaryBox" VBox.vgrow="ALWAYS" />
            <HBox fx:id="secondaryBox" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
